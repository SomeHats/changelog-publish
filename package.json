{
  "name": "changelog-publish",
  "version": "1.0.0",
  "description": "Move unreleased changes from CHANGELOG.md into a new release",
  "main": "index.js",
  "scripts": {
    "pretest": "flow",
    "version": "./bin/changelog-publish && prettier --write CHANGELOG.md && git add CHANGELOG.md",
    "release": "np",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "bin": {
    "changelog-publish": "./bin/changelog-publish"
  },
  "author": "Alex Dytrych <alex@dytry.ch>",
  "license": "MIT",
  "devDependencies": {
    "flow-bin": "^0.101.0",
    "jest": "^24.8.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "mdast-util-to-string": "^1.0.6",
    "remark": "^10.0.1"
  }
}
